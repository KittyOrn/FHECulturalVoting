{
  "name": "privacy-voting-platform",
  "version": "1.0.0",
  "description": "Privacy-Protected Cultural Project Voting System with FHE",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "lint": "npm run lint:sol && npm run lint:js",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:sol:fix": "solhint 'contracts/**/*.sol' --fix",
    "lint:js": "eslint '**/*.js'",
    "lint:js:fix": "eslint '**/*.js' --fix",
    "format": "prettier --write '**/*.{js,json,sol,md}'",
    "format:check": "prettier --check '**/*.{js,json,sol,md}'",
    "security:check": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deploy": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:local": "hardhat run scripts/deploy.js --network hardhat",
    "verify": "hardhat run scripts/verify.js --network sepolia",
    "interact": "hardhat run scripts/interact.js --network sepolia",
    "simulate": "hardhat run scripts/simulate.js --network hardhat",
    "dev": "npx http-server . -p 3000 -c-1 --cors",
    "clean": "hardhat clean",
    "prepare": "husky install"
  },
  "keywords": [
    "blockchain",
    "voting",
    "cultural",
    "privacy",
    "fhevm",
    "hardhat",
    "ethereum"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "chai": "^4.5.0",
    "eslint": "^8.57.0",
    "ethers": "^6.15.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.10",
    "http-server": "^14.1.1",
    "husky": "^9.0.0",
    "prettier": "^3.2.0",
    "prettier-plugin-solidity": "^1.3.0",
    "solhint": "^5.0.0",
    "solidity-coverage": "^0.8.16"
  }
}